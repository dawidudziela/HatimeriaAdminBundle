<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <title>{% block title %}Hatimeria CMF{% endblock %}</title>
        {% render "HatimeriaExtJSBundle:Default:headers" %}
        <script type="text/javascript">
            Ext.Loader.setPath('HatimeriaAdmin', '/bundles/hatimeriaadmin/js');
        </script>
        {% render "HatimeriaAdminBundle:Default:headers" %}
        {% block head %}{% endblock %}
        <link rel="shortcut icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        <div id="body"></div>
        {% block body %}{% endblock %}
        {% block fos_user_content %}{% endblock %}
        {% block javascripts %}{% endblock %}
        {% render "HatimeriaExtJSBundle:Default:variables" %}
    </body>
</html>
